{% extends 'partials/base.html.twig' %}
{% block javascripts %}
  {{ parent() }}
  {% do assets.addJs('theme://js/app.js', {'priority': 100, 'group':'bottom'}) %}
  {% do assets.addJs('theme://js/background-inner.js', {'priority': 99, 'group':'bottom'}) %}
{% endblock %}
{% block body %}
<div class="c-header">
<div class="c-site-identity c-site-identity--header" href="{{ home_url }}">
    Andrea Crawford
</div>
{% include 'partials/mainmenu.html.twig' with {'classes':'c-menu--header'} %}
</div>
  <div class="c-sidebar">
    <a class="c-site-identity c-site-identity--sidebar" href="{{ home_url }}">Andrea Crawford</a>
    {% include 'partials/mainmenu.html.twig' with {'classes':'c-menu--sidebar'} %}
    {% block sidebar %}
    {% endblock %}
  </div>
  <div class="c-container--sidebar">
  {% block content %}{% endblock %}
  </div>
{% endblock %}
