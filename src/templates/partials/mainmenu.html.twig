<ul class="c-menu {{ classes }}">
    {% for mitem in site.menu %}
        <li>
            <a href="{{ site.url }}{{ mitem.url }}">
                <span>{% if mitem.icon %}<i class="fa fa-{{ mitem.icon }}"></i>{% endif %}
                {{ mitem.text }}</span>
            </a>
        </li>
    {% endfor %}
</ul>
